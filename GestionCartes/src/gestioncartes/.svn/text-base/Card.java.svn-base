package gestioncartes;

/**
 *
 * @author fortj04
 */
public class Card
{
    private String _title;
    private String _team;
    private int _year;
    /**
     * Card constructor
     * @param title
     * @param team
     * @param year
     */
    Card(String title, String team, int year)
    {
        _title = title;
        _team = team;
        _year = year;
    }
    /**
     * Return the card title
     * @return
     */
    String getTitle()
    {
        return _title;
    }
    /**
     * Return the card team
     * @return
     */
    String getTeam()
    {
        return _team;
    }
    /**
     * Return the year of the card
     * @return
     */
    int getYear()
    {
        return _year;
    }
}
